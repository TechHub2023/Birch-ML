struct Expm1<Middle>(m:Middle) < Unary<Middle>(m) {
  /**
   * Memoized result.
   */
  phantom x;

  hpp{{
  BIRCH_UNARY_FUNCTION_FORM(expm1)
  }}
}

function grad<Middle,Gradient>(x:Expm1<Middle>, d:Gradient) {
  grad(x.m, d*exp(peek(x.m)));
}

