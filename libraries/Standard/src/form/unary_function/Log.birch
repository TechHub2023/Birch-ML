struct Log<Middle>(m:Middle) < Unary<Middle>(m) {
  /**
   * Memoized result.
   */
  phantom x;

  hpp{{
  BIRCH_UNARY_FUNCTION_FORM(log)
  }}
}

function grad<Middle,Gradient>(x:Log<Middle>, d:Gradient) {
  grad(x.m, hadamard(d, rcp(peek(x.m))));
}

