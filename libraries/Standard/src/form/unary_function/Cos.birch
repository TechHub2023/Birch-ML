struct Cos<Middle>(m:Middle) < Unary<Middle>(m) {
  /**
   * Memoized result.
   */
  phantom x;

  hpp{{
  BIRCH_UNARY_FUNCTION_FORM(cos)
  }}
}

function grad<Middle,Gradient>(x:Cos<Middle>, d:Gradient) {
  grad(x.m, -d*sin(peek(x.m)));
}

