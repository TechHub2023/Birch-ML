struct LDet<Middle>(m:Middle) < Unary<Middle>(m) {
  /**
   * Memoized result.
   */
  phantom x;

  hpp{{
  BIRCH_UNARY_FUNCTION_FORM(ldet)
  }}
}

function grad<Middle,Gradient>(x:LDet<Middle>, d:Gradient) {
  grad(x.m, d*transpose(inv(peek(x.m))));
}

hpp{{
namespace birch {
using numbirch::ldet;

/**
 * Log-determinant of a matrix.
 */
template<class Middle, std::enable_if_t<is_delay<Middle>::value,int> = 0>
LDet<Middle> ldet(const Middle& m) {
  return construct<LDet<Middle>>(m);
}
}
