/**
 * Iterator over a Tape.
 *
 * - head: Head node.
 */
final class TapeIterator<Type>(head:TapeNode<Type>?) < Iterator<Type> {
  /**
   * Current node.
   */
  node:TapeNode<Type>? <- head;

  /**
   * Is there a next element?
   */
  function hasNext() -> Boolean {
    return node?;
  }

  /**
   * Get the next element.
   */
  function next() -> Type {
    let x <- node!.x;
    node <- node!.next;
    return x;
  }
}
