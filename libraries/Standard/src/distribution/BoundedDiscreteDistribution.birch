/**
 * Bounded discrete distribution.
 */
abstract class BoundedDiscreteDistribution < DiscreteDistribution {
  override function isSecondOrder() -> Boolean {
    return true;
  }

  override function isBoundedDiscrete() -> Boolean {
    return true;
  }

  override function getBoundedDiscrete() -> BoundedDiscreteDistribution? {
    return this;
  }
}
