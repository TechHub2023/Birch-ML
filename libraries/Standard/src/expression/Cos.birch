/**
 * Lazy `cos`.
 */
final class Cos(m:Expression<Real>) <
    ScalarUnaryExpression<Expression<Real>,Real>(m) {
  override function doCompute() {
    x <- cos(m!.eval());
  }

  override function doGrad(gen:Integer) {
    m!.countGrad(gen, -d!*sin(m!.eval()));
  }
}

/**
 * Lazy `cos`.
 */
function cos(m:Expression<Real>) -> Cos {
  return construct<Cos>(m);
}
