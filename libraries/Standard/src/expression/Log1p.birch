/**
 * Lazy `log1p`.
 */
final class Log1p(m:Expression<Real>) <
    ScalarUnaryExpression<Expression<Real>,Real>(m) {
  override function doCompute() {
    x <- log1p(m!.eval());
  }

  override function doGrad(gen:Integer) {
    m!.countGrad(gen, d!/(1.0 + m!.eval()));
  }
}

/**
 * Lazy `log1p`.
 */
function log1p(m:Expression<Real>) -> Log1p {
  return construct<Log1p>(m);
}
