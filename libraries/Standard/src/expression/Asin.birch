/**
 * Lazy `asin`.
 */
final class Asin(m:Expression<Real>) <
    ScalarUnaryExpression<Expression<Real>,Real>(m) {
  override function doCompute() {
    x <- asin(global.eval(m!));
  }

  override function doGrad(gen:Integer) {
    count_grad(m!, gen, d!/sqrt(1.0 - pow(global.eval(m!), 2.0)));
  }
}

/**
 * Lazy `asin`.
 */
function asin(m:Expression<Real>) -> Asin {
  return construct<Asin>(m);
}
