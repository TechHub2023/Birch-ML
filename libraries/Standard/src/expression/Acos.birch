/**
 * Lazy `acos`.
 */
final class Acos(m:Expression<Real>) <
    ScalarUnaryExpression<Expression<Real>,Real>(m) {
  override function doCompute() {
    x <- acos(m!.eval());
  }

  override function doGrad(gen:Integer) {
    m!.countGrad(gen, -d!/sqrt(1.0 - pow(m!.eval(), 2.0)));
  }
}

/**
 * Lazy `acos`.
 */
function acos(m:Expression<Real>) -> Acos {
  return construct<Acos>(m);
}
