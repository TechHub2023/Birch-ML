/**
 * Lazy `atan`.
 */
final class Atan(m:Expression<Real>) <
    ScalarUnaryExpression<Expression<Real>,Real>(m) {
  override function doCompute() {
    x <- atan(global.eval(m!));
  }

  override function doGrad(gen:Integer) {
    count_grad(m!, gen, d!/(1.0 + pow(global.eval(m!), 2.0)));
  }
}

/**
 * Lazy `atan`.
 */
function atan(m:Expression<Real>) -> Atan {
  return construct<Atan>(m);
}
