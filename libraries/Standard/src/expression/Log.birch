/**
 * Lazy `log`.
 */
final class Log(m:Expression<Real>) <
    ScalarUnaryExpression<Expression<Real>,Real>(m) {
  override function doCompute() {
    x <- log(m!.eval());
  }

  override function doGrad(gen:Integer) {
    m!.countGrad(gen, d!/m!.eval());
  }
}

/**
 * Lazy `log`.
 */
function log(m:Expression<Real>) -> Log {
  return construct<Log>(m);
}
