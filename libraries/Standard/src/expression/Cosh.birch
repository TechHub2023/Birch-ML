/**
 * Lazy `cosh`.
 */
final class Cosh(m:Expression<Real>) <
    ScalarUnaryExpression<Expression<Real>,Real>(m) {
  override function doCompute() {
    x <- cosh(m!.eval());
  }

  override function doGrad(gen:Integer) {
    m!.countGrad(gen, -d!*sinh(m!.eval()));
  }
}

/**
 * Lazy `cosh`.
 */
function cosh(x:Expression<Real>) -> Cosh {
  return construct<Cosh>(x);
}
