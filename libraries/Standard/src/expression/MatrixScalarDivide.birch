/**
 * Lazy matrix divide.
 */
operator (l:Expression<Real[_,_]>/r:Expression<Real>) ->
    Expression<Real[_,_]> {
  return (1.0/r)*l;
}

/**
 * Lazy matrix divide.
 */
operator (l:Real[_,_]/r:Expression<Real>) -> Expression<Real[_,_]> {
  return (1.0/r)*box(l);
}

/**
 * Lazy matrix divide.
 */
operator (l:Expression<Real[_,_]>/r:Real) -> Expression<Real[_,_]> {
  return box(1.0/r)*l;
}
