program test_pdf_poisson(N:Integer <- 10000, B:Integer <- 0, S:Integer <- 0,
    lazy:Boolean <- false) {
  let 位 <- simulate_uniform(0.1, 100.0);
  let q <- Poisson(位);
  test_pdf(q, N, lazy);
}

program test_cdf_poisson() {
  let 位 <- simulate_uniform(0.1, 100.0);
  let q <- Poisson(位);
  test_cdf(q);
}
