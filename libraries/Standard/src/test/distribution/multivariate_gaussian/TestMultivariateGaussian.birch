program test_pdf_multivariate_gaussian(D:Integer <- 4, N:Integer <- 10000,
    B:Integer <- 1000, S:Integer <- 20, lazy:Boolean <- false) {
  μ:Real[D];
  Σ:Real[D,D];

  for i in 1..D {
    μ[i] <- simulate_uniform(-10.0, 10.0);
    for j in 1..D {
      Σ[i,j] <- simulate_uniform(-2.0, 2.0);
    }
  }
  Σ <- outer(Σ);

  let π <- MultivariateGaussian(μ, Σ);
  test_pdf(π, N, B, S, lazy);
}
