program test_pdf_exponential(N:Integer <- 10000, B:Integer <- 100,
    S:Integer <- 5, lazy:Boolean <- false) {
  let 位 <- simulate_uniform(1.0, 10.0);
  let q <- Exponential(位);  
  test_pdf(q, N, B, S, lazy);
}

program test_cdf_exponential(N:Integer <- 10000) {
  let 位 <- simulate_uniform(1.0, 10.0);
  let q <- Exponential(位);  
  test_cdf(q, N);
}
