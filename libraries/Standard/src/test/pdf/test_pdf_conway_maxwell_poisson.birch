/*
 * Test Conway-Maxwell-Poisson pdf evaluations.
 */
program test_pdf_conway_maxwell_poisson(N:Integer <- 10000, B:Integer <- 0,
    S:Integer <- 0, lazy:Boolean <- false) {
  let λ <- simulate_uniform(0.1, 100.0);
  let ν <- simulate_uniform(0.1, 20.0);
  let n <- simulate_uniform_int(scalar<Integer>(10*λ), scalar<Integer>(20*λ));
  let q <- ConwayMaxwellPoisson(λ, ν, n);
  test_pdf(q, N, lazy);
}
